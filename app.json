{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload it to your profile."
        }
      ]
    ],
    "name": "EventsHub",
    "slug": "EventsHub",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe76e787-e237-4d03-b487-6a7abb06eb84"
      }
    },
    "android": {
      "package": "com.purushotham11.EventsHub",
      "googleServicesFile": "./google-services.json"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fe76e787-e237-4d03-b487-6a7abb06eb84"
    },
    "ios": {}
  }
}
